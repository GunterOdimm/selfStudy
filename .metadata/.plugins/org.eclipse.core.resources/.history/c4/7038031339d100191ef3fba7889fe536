package study.java.code;

public class kakaotest02 {
	public static void main(String[] args) {
		int answer = 0;
		int countOfOp = 0;
		int countOfCl = 0;

		String p = "(()())()";
		String[] userInputArray;
		userInputArray = p.split("");

		for(int i = 0; i < userInputArray.length; i++) {
			if(userInputArray[i].equals("(")){
				countOfOp++;
			}else {
				countOfCl++;
			}
		}
	}
}
