package study.java.code;

public class sum {
	public static void main(String[] args) {
		int a = 3;
		int b = 5;
		long answer = 0;
		System.out.println(b);
		for (int i = 0; i < 10000000; i++) {
			if (a + i < b) {
				answer += a + i;
				System.out.println(answer);
			} else if (a + i == b) {
				System.out.println(b);
				answer += b;
			} else if (a == b) {
				answer = a;
			}
		}

		System.out.println(answer);
	}
}
